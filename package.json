{
  "name": "recmu-api",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.js",
  "scripts": {
    "start": "nodemon src/index.js",
    "test": "jest --coverage"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.js",
      "!<rootDir>/node_modules/"
    ],
    "coverageReporters": ["text"],
    "coverageThreshold": {
      "global": {
        "branches": 50,
        "functions": 85,
        "lines": 85,
        "statements": 85
      },
      "./src/**/*.js": {
        "lines": 85
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MartinMolinero/recmu-api.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MartinMolinero/recmu-api/issues"
  },
  "homepage": "https://github.com/MartinMolinero/recmu-api#readme",
  "dependencies": {
    "axios": "^1.4.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "eslint": "^8.43.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^16.0.1",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^25.0.0",
    "nodemon": "^1.14.9"
  }
}
